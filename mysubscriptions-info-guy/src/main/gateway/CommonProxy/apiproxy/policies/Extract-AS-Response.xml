            <JSONPath>$.authGroups</JSONPath>
        </Variable>
        <Variable name="asServiceAccess">
            <JSONPath>$.serviceAccess</JSONPath>
        </Variable>
        <Variable name="asDeviceId">
            <JSONPath>$.deviceId</JSONPath>
        </Variable>
        <Variable name="asDlProfileId">
